  "objects": {
    "dataPoint": {
      "displayName": "Indego Visual Settings",
      "properties": {


        "speed": {
          "displayName": "Animation Speed (high value is slower speed, 1000 is default",
          "description": "Animation Speed",
          "type": {
            "numeric": true
          }
        },
        "SVG_width": {
          "displayName": "SVG Width",
          "description": "width",
          "type": {
            "numeric": true
          }
        },

        "SVG_height": {
          "displayName": "SVG Height",
          "description": "Height",
          "type": {
            "numeric": true
          }
        }

      }
    }
  },

  "dataViewMappings": [
    {
      "categorical": {
        "categories": {
          "select": [
            {
              "bind": {
                "to": "I"
              }
            },
            {
              "bind": {
                "to": "X"
              }
            },
            {
              "bind": {
                "to": "Y"
              }
            },
            {
              "bind": {
                "to": "DT"
              }
            },
            {
              "bind": {
                "to": "ET"
              }
            },
            {
              "bind": {
                "to": "SVG"
              }
            }
          ],
          "dataReductionAlgorithm": {
            "top": {
              "count": 30000
            }
          }
        },
        "values": {
          "select": [
            {
              "bind": {
                "to": "measure"
              }
            }
          ]
        }
      },
      "table": {
        "rows": {
          "select": [
            {
              "for": {
                "in": "column"
              }
            },
            {
              "for": {
                "in": "value"
              }
            }
          ]
        }
      }
    }
  ]
